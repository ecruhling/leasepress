{
	"name": "leasepress",
	"version": "1.0.0",
	"description": "A WordPress plugin to integrate with RENTCafe's API",
	"author": "Erik Ruhling <erik@resourceatlanta.com>",
	"license": "UNLICENSED",
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"devDependencies": {
		"@tinypixelco/laravel-mix-wp-blocks": "^1.1.0",
		"@wordpress/babel-preset-default": "^4.19.0",
		"@wordpress/browserslist-config": "^2.7.0",
		"@wordpress/dependency-extraction-webpack-plugin": "^2.8.0",
		"babel-eslint": "^10.1.0",
		"browser-sync": "^2.26.13",
		"browser-sync-webpack-plugin": "^2.2.2",
		"coffee-loader": "^1.0.1",
		"coffeescript": "^2.5.1",
		"cross-env": "^7.0.2",
		"eslint": "^7.13.0",
		"eslint-plugin-import": "^2.22.1",
		"grunt": "*",
		"grunt-contrib-coffee": "2.x",
		"grunt-contrib-compass": "*",
		"grunt-contrib-watch": "*",
		"laravel-mix": "^5.0.9",
		"laravel-mix-copy-watched": "^2.2.4",
		"laravel-mix-purgecss": "^5.0.0",
		"npm-run-all": "^4.1.5",
		"purgecss-with-wordpress": "^3.0.0",
		"rimraf": "^3.0.2",
		"sass": "^1.29.0",
		"sass-loader": "^10.1.0",
		"stylelint": "^13.8.0",
		"stylelint-config-standard": "^20.0.0",
		"vue-template-compiler": "^2.6.12"
	},
	"scripts": {
	  "build": "cross-env NODE_ENV=development run-s mix",
	  "build:production": "cross-env NODE_ENV=production run-s clean mix",
	  "start": "cross-env NODE_ENV=development run-s \"mix -- --watch\"",
	  "hot": "cross-env NODE_ENV=development run-s build mix:hot",
	  "mix": "webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
	  "mix:hot": "webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
	  "clean": "run-p clean:*",
	  "clean:dist": "rimraf dist",
	  "lint": "run-s -c lint:*",
	  "lint:scripts": "eslint assets/js",
	  "lint:styles": "stylelint \"assets/**/*.{vue,css,sass,scss,less}\"",
	  "test": "run-s -c lint"
	},
	"dependencies": {
	  "jquery": "^3.5.1",
	  "svg-injector": "^1.1.3"
	}
}
